{
  "$schema": "http://json.schemastore.org/package",
  "name": "wmfnext-fakes",
  "version": "0.0.1",
  "description": "Fake implementations to faciliate the development of federated modules in isolation",
  "author": "Patrick Lafrance",
  "license": "Apache-2.0",
  "repository": "https://github.com/patricklafrance/wmfnext-shell",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "engines": {
    "node": ">=19"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc --build ./tsconfig.build.json",
    "clean": "run-p --continue-on-error delete-dist delete-ts-cache unlink-pkg",
    "delete-dist": "rimraf dist",
    "delete-ts-cache": "rimraf *.tsbuildinfo",
    "dev": "tsc --watch --project ./tsconfig.build.json",
    "setup": "yarn link",
    "unlink-pkg": "yarn unlink"
  },
  "dependencies": {
    "wmfnext-shell": "0.0.1"
  }
}
